{
  "name": "",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Helps you choose the right emoji",
  "homepage_url": "http://example.com/",
  "icons": {
    "16": "icons/icon16.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery.min.js",
        "js/textcomplete.min.js",
        "src/inject.js"
      ],
      "css": [
        "css/main.css"
      ]
    }
  ]
}